    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Debug Project",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/main_pyside.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "python": "${workspaceFolder}/venv/bin/python"
        },
        {
            "name": "Run Fuzz Loop (Phoenix)",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/tests/run_fuzz_loop.py",
            "console": "integratedTerminal",
            "justMyCode": false
        }
    ]
}
